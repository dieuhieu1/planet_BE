PORT=3000

# Database Config
DB_HOST=127.0.0.1
DB_USER=postgres
DB_PASS=your_password
DB_NAME=planet_web_db
DB_DIALECT=postgres

# JWT Config
JWT_ACCESS_KEY=your_super_secret_access_key
JWT_REFRESH_KEY=your_super_secret_refresh_key
